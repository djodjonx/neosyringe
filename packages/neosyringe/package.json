{
  "name": "@djodjonx/neosyringe",
  "version": "0.1.0",
  "description": "Runtime library for NeoSyringe DI",
  "type": "module",
  "license": "MIT",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.mts",
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsdown",
    "test": "vitest run"
  },
  "devDependencies": {
    "tsdown": "0.20.0-beta.3",
    "typescript": "^5.9.3",
    "vitest": "^4.0.17"
  }
}
