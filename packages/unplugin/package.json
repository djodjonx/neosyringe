    "unplugin": "^2.0.0"
  },
  "peerDependencies": {
    "typescript": ">=5.0.0"
  },
  "devDependencies": {
    "tsdown": "0.20.0-beta.3",
    "typescript": "^5.9.3",
    "vitest": "^4.0.17"
  }
}
