{
  "name": "@djodjonx/neosyringe-plugin",
  "version": "0.0.6",
  "description": "NeoSyringe build plugin (Vite, Rollup, Webpack)",
  "type": "module",
  "license": "MIT",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
